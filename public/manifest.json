{
    "manifest_version": 3,
    "name": "BookmarkTab",
    "version": "0.1.0",
    "description": "Display all bookmarks in your new tab for easy access",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "icons": {
        "16": "icon.svg",
        "48": "icon.svg",
        "128": "icon.svg"
    }
}